{
  "family": "documentportaltd",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::859037107780:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "documentportal",
      "image": "859037107780.dkr.ecr.ap-south-1.amazonaws.com/documentportal",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "document-portal-container-8080-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "secrets": [
        {
          "name": "OPENAI_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:859037107780:secret:api_keys-0cXcYT:OPENAI_API_KEY"
        },
        {
           "name": "PROVIDER_NAME",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:859037107780:secret:api_keys-0cXcYT:PROVIDER_NAME"
        },
                {
           "name": "GOOGLE_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:859037107780:secret:api_keys-0cXcYT:GOOGLE_API_KEY"
        },
                {
           "name": "GROQ_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:859037107780:secret:api_keys-0cXcYT:GROQ_API_KEY"
        }
        
        
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/documentportaltd",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
